<?xml version="1.0" encoding="UTF-8"?>
<!-- Ivy won't let us specify private repository in ivy.xml, so let's create ivysettings.xml for it. -->
<ivysettings>
	<settings defaultResolver="default" />
	<include url="${ivy.default.settings.dir}/ivysettings-public.xml"/>
	<resolvers>
		<chain name="default" changingPattern=".*SNAPSHOT" returnFirst="true">
			<!-- Public repository resolver goes first. It speeds things up. -->
			<resolver ref="public" />
			<!-- PushMode is served out of private maven repository. -->
			<ibiblio name="pushmode" m2compatible="true" root="https://cdnedge.angeloflogic.com/maven/releases" />
		</chain>
	</resolvers>
</ivysettings>
